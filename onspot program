#include <stdio.h>//head filer//

//main function//
int main() 
{
  char sex;
  int age, ID;
  printf("Enter Sex: ");
  scanf("%c", &sex);
  printf("Enter ID no.: ");
  scanf("%d", &ID);
  printf("Enter age: ");
  scanf("%d",&age);
  
  //to determine if the voter id eligible to vote or not//
  if(age>=18)
  {
      printf("You are eligible for Voting!");
  }
  else
  {
      printf(" Sorry! You are not eligible for Voting!");
  }
  //close program//
  return 0;

}
